# Database configuration
# Options: 'qdrant', 'chroma'
DATABASE_TYPE=qdrant

# Common settings
COLLECTION_NAME=your_collection_name
HTTP_SERVER=false
PORT=3000

# Qdrant settings
QDRANT_URL=https://your-qdrant-instance-url.example.com:6333
QDRANT_API_KEY=your_qdrant_api_key
# Using Qdrant's built-in FastEmbed for embeddings

# Chroma settings
CHROMA_URL=http://localhost:8000

# Gemini settings
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-embedding-001
# Optional: Reduce embedding dimensions for compatibility with existing collections
# Default is 3072 for gemini-embedding-001. Set to 768 if your collection expects that size.
# GEMINI_EMBEDDING_DIMENSIONS=768
